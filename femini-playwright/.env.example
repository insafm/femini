# Multiple Google credentials (JSON array format)
GEMINI_CREDENTIALS='[
  {
    "email": "user1@gmail.com",
    "password": "password1",
    "key": "account1"
  },
  {
    "email": "user2@gmail.com",
    "password": "password2",
    "key": "account2"
  },
  {
    "email": "user3@gmail.com",
    "password": "password3",
    "key": "account3"
  }
]'

# Credential selection mode: random, round_robin, default, least_busy
CREDENTIAL_MODE=random

# Default credential index (used when mode=default)
DEFAULT_CREDENTIAL_INDEX=0

# Concurrency settings
MAX_CONCURRENT_PER_CREDENTIAL=1
MAX_TOTAL_CONCURRENT=3

# Browser settings
HEADLESS=true
REQUEST_TIMEOUT=180
BROWSER_CONTEXT_TIMEOUT=300

# Logging
LOG_LEVEL=INFO

# Paths (relative to project root)
USER_DATA_BASE_DIR=user_data
COOKIES_BASE_DIR=cookies
DOWNLOAD_DIR=downloads
LOG_DIR=logs

# Gemini settings
BASE_URL=https://gemini.google.com/app?hl=en-IN
MAX_TIMEOUT=180
TIMEOUT=60
MAX_RETRIES=5

# Image data settings
RETURN_IMAGE_DATA=false  # Return base64-encoded image data in response (increases memory usage)
